                                            <Setter Property="Margin" Value="0" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding Tag, RelativeSource={RelativeSource Mode=TemplatedParent}, Mode=OneWay}" Value="non-success-dark">
                                                    <Setter Property="Background" Value="Black" />

                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding WindowState, RelativeSource={RelativeSource Mode=TemplatedParent}, Mode=OneWay}" Value="Maximized">

                                                    <Setter Property="Margin" Value="1,1,4,1" />

                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Grid.Style>
                                    <Grid>
                                        <Grid.Style>
                                            <Style TargetType="Grid">
                                                <Setter Property="Margin" Value="3,1,3,3" />
